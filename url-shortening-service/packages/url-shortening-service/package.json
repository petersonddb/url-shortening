{
  "name": "url-shortening-service",
  "description": "Create and redirect short links to longer URLs",
  "author": "peterson.bem",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petersonddb/url-shortening.git"
  },
  "bugs": {
    "url": "https://github.com/petersonddb/url-shortening/issues"
  },
  "scripts": {
    "migrate-up": "migrate-mongo up",
    "migrate-down": "migrate-mongo down",
    "migrate-status": "migrate-mongo status",
    "protoc-js": "grpc_tools_node_protoc -I../../../services-contracts --js_out=import_style=commonjs,binary:./src/keygen-service --grpc_out=grpc_js:./src/keygen-service ../../../services-contracts/keys-contract.proto ",
    "protoc-ts": "grpc_tools_node_protoc -I../../../services-contracts --ts_out=grpc_js:./src/keygen-service ../../../services-contracts/keys-contract.proto",
    "dev": "tsc && node --env-file=.env ./dist/index.js",
    "build": "tsc",
    "lint": "eslint .",
    "test": "vitest run",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "google-protobuf": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@faker-js/faker": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.0",
    "@types/supertest": "^6.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.13.0",
    "migrate-mongo": "^12.1.3",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  }
}
